<!-- Spring-Common.xml -->
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
 
	<bean id="OutputHelper" class="com.bigshi.mkyong.spring.OutputHelper">
		<property name="outputGenerator" ref="CsvOutputGenerator"></property>
	</bean>
	
	<bean id="CsvOutputGenerator" class="com.bigshi.mkyong.spring.CsvOutputGenerator" />
	<bean id="JsonOutputGenerator" class="com.bigshi.mkyong.spring.JsonOutputGenerator" />

</beans>